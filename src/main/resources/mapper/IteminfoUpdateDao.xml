<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.cq.IteminfoUpdateDao">  
	<update id="updateIteminfo" parameterType="com.entity.Iteminfo">
		update iteminfo set item_status = #{itemStatus} where item_id = #{itemId}
	</update>
</mapper>